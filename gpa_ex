#include <bits/stdc++.h>
using namespace std;
int main(){
   int x;
   scanf("%d", &x);

    if(x<40){
        printf("0.8");
    }
    else if(x<=44){
        printf("1.2");
    }
    else if(x<=49){
        printf("1.6");
    }
    else if(x<=54){
        printf("2.0");
    }
    else if(x<=59){
        printf("2.4");
    }
    else if(x<=64){
        printf("2.8");
    }
    else if(x<=69){
        printf("3.2");
    }
    else if(x<=79){
        printf("3.6");
    }
    else {
        printf("4.0");
    }
}
