#include <bits/stdc++.h>
#include <ctype.h>
#include <string.h>
using namespace std;
#define int long long

int32_t main() {
    int n, biggest = 0;
    string biggest_word = "";
    cin >> n;
    string str[n];
    for (int i = 0; i < n; i++) {
        cin >> str[i];
        if (str[i].length() > biggest) {
            biggest_word = str[i];
            biggest = str[i].length();
        }
    }
    cout << biggest_word;
}
