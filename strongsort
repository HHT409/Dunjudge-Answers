#include <bits/stdc++.h>
using namespace std;
#define int long long

main() {
    string str;
    getline(cin, str);
    string upper = "";
    string lower = "";
    for (int i = 0; i < str.size(); i++) {
        if (isalpha(str[i])) {
            if (isupper(str[i])) {
                upper += str[i];
            } else {
                lower += str[i];
            }
        }
    }
    cout << upper << lower;
}
