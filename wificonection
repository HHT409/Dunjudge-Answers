#include <iostream>
#include <bits/stdc++.h>
using namespace std;
#define int long long

main() {
    int n, m, x, y, sum = 0;
    cin >> n >> m;
    for (int i = 0; i < n; i++) {
        cin >> x >> y;
        if (ceil(sqrt(x * x + y * y)) <= m) {
            sum++;
        }
    }
    cout << sum;
}
