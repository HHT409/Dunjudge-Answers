#include <bits/stdc++.h>
#include <sstream>
using namespace std;

main() {
    long long n;
    string m;
    cin >> n;
    for (long long i = 0; i < n; i++) {
        cin >> m;
        int k = m[m.size() - 1] - '0';
        if (k % 2 == 0) {
            cout << "even\n";
        } else {
            cout << "odd\n";
        }
    }
}
