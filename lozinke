//40
#include<bits/stdc++.h>
using namespace std;
#define int long long

int32_t main() {
    int n, sum = 0;
    cin >> n;
    string str[n];
    for (int i = 0; i < n; i++) {
        cin >> str[i];
    }
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            if (i != j) {
                if (str[i].find(str[j]) != string::npos) {
                    sum++;
                }
            }
        }
    }
    cout << sum;
}
