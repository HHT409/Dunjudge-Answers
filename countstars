#include <bits/stdc++.h>
using namespace std;

int32_t main() {
	ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
	long long n, m, stars = 0;
	cin >> n >> m;
	char sky[1000][1000];

	for (int i = 0; i < n; i++) {
		for (int p = 0; p < m; p++) {
			cin >> sky[i][p];
			if (sky[i][p] == '*') stars++;
		}
	}
	cout << stars;
}
