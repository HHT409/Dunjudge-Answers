#include <bits/stdc++.h>
using namespace std;
#define int long long

int32_t main() {
    int n;
    cin >> n;
    for (int i = 0; i < 2 * n; i++) {
        for (int j = 0; j < 2 * n; j++) {
            cout << min(min(j + 1, i + 1), min(2 * n - i, 2 * n - j));
        }
        cout << '\n';
    }
}
